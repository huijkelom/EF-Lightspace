<UserControl 
    x:Class="LightSpace_WPF_Engine.Wpf.Views.UserControls.TileLayoutUserControl"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
    xmlns:local="clr-namespace:LightSpace_WPF_Engine.Wpf.Views.UserControls"
    mc:Ignorable="d" 
    >

    <Grid Background="{StaticResource LightSpaceGray_Dark}">
        <Viewbox Name="RightViewViewBox" Stretch="Uniform">
            <Canvas Name="RightViewControl" Height="0" Width="0" />
        </Viewbox>
    </Grid>
</UserControl>
